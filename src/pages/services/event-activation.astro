---
import ServicesLayout from '../../layouts/ServicesLayout.astro'
import PanelContentBlock from '../../components/pages/services/PanelContentBlock.astro'
import services from '../../../public/services.json'
import eventImage from '../../assets/party.jpg'

const eventActivationService = services[1]
---

<ServicesLayout title="66th & Byrnes | Event Activation" description="Event Activation">
  <PanelContentBlock
    id="high-quality-events"
    title="High Quality Events"
    description={eventActivationService.description}
    image={eventImage}
    imageAlt="High Quality Events Image"
  />
</ServicesLayout>
